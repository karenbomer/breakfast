<form method="post" action="{{ path('app_backoffice_category_delete', {'id': category.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.id) }}">
    <button class="btn btn-outline-light d-flex text-uppercase">Supprimer</button>
</form>
